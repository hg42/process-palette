{
  "patterns": {
    "colon": {
      "expression": "(path):(line)"
    },
    "file_line": {
      "expression": "(?:\\bfile +|\\bat +)?['\"]?(path)['\"]?.*\\bline +(line)"
    },
    "path": {
      "expression": "['\"]?(path)['\"]?"
    },
    "fail": {
      "expression": "\\bfail(ure|ed)?\\b"
    },
    "error": {
      "expression": "[-\\w]*error[-\\w]*"
    },
    "warning": {
      "expression": "\\bwarn(ing)?\\b"
    },
    "dashes": {
      "expression": "^\\s*--"
    },
    "command": {
      "expression": "^\\s*>"
    },
    "star": {
      "expression": "^\\s*\\*"
    },
    "trace-begin": {
      "expression": "^\\s*\\\\\\s+"
    },
    "trace-end": {
      "expression": "^\\s*/\\s+"
    },
    "trace-result": {
      "expression": "(^\\s*/\\s+.*)(\\s+=\\s+.*)(.*$)"
    },
    "exclamation": {
      "expression": "^\\s*!"
    },
    "bracket": {
      "expression": "^\\s*[[{]"
    },
    "build": {
      "expression": "^\\s*build"
    },
    "at": {
      "expression": "^\\s*at\\b"
    }
  },
  "defaultPatterns": [
    "colon",
    "file_line",
    "path",
    "default",
    "trace-begin",
    "trace-end",
    "trace-result",
    "exclamation",
    "star",
    "dashes",
    "command",
    "bracket",
    "build",
    "at",
    "fail",
    "error",
    "warning"
  ],
